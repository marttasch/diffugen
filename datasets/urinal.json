{
    "dataset_name": "urinal_with_TI_1",
    "dataset_root": "datasets_output",
    "image_format": "png",
    "image_size": {"width": 512, "height": 512},
    "steps": 50,
    "dataset_classes": ["pissoir_clean", "pissoir_average", "pissoir_dirty"],
    "dataset_to_model_class_map": {"pissoir_clean": "toilet", "pissoir_average": "toilet", "pissoir_dirty": "toilet"},
    "label_model": {"type": "YOLOSeg", "weights": "models/seg/yolov8l-seg.pt", "classes": "models/seg/yolov8-classes.txt"},
    "unsupervised": true,
    "deterministic": true,
    "images_per_shard": 500,
    "textual_inversion": {
        "weights":"models/sd/embeddings/urinal-embedding-SDv15_v4-500-2500.pt",
        "keyword": "<urinalEmbedding>"
    },
    "txt2img": {
        "stable_diffusion_model": "SG161222/Realistic_Vision_V5.1_noVAE",
        "seed_range": {"min":9092091, "max":9982091},
        "seed_count_per_prompt": 1,
        "object_names": ["urinal"],
        "view_points": ["front", "side", "top", "close-up", "distant"],
        "times_of_day": ["day"],
        "sky_conditions": ["clear"],
        "weather_conditions": ["dry"],
        "prompts": [
            "a photo of a {object_name} <urinalEmbedding>++ mounted on a wall in a bathroom. clean condition. {view_point} view",
            "a photo of a {object_name} <urinalEmbedding>++ mounted on a wall in a bathroom. average condition. {view_point} view",
            "a photo of a {object_name} <urinalEmbedding>++ mounted on a wall in a bathroom. in dirty condition. {view_point} view"
        ],
        "negative_prompt": "(deformed toilet, deformed urinal, broken toilet, broken urinal)"
    }
}
